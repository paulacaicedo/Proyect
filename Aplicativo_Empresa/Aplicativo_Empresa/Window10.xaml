            <Label Content="Precio Total" HorizontalAlignment="Left" Margin="511,187,0,0" VerticalAlignment="Top" Width="93" FontFamily="Times New Roman" FontSize="15"/>
            <TextBox x:Name="textbox_materials" HorizontalAlignment="Left" Height="94" Margin="331,293,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190" FontFamily="Arial"/>
            <Label Content="Materiales" HorizontalAlignment="Left" Margin="331,266,0,0" VerticalAlignment="Top" Width="126" FontFamily="Times New Roman" FontSize="15"/>
